<launch>
    <include file="$(find rplidar_ros)/launch/rplidar.launch" />
    <node pkg="low_level_interface" name="odom_pose_transformer" type="SVEA1_to_odom.py" output="screen">
    </node>
    <node pkg="tf" name="static_transformer_A" type="static_transform_publisher" output="screen">
      <param name="x" value="-0.1" />
      <param name="y" value="0" />
      <param name="z" value="0" />
      <param name="yaw" value="0" />
      <param name="roll" value="0" />
      <param name="pitch" value="0" />
      <param name="frame_id" value="laser" />
      <param name="child_frame_id" value="SVEA1" />
      <param name="period_in_ms" value="100" />
    </node>
    <node pkg="gmapping" name="slam_gmapping" type="slam_gmapping" output="screen">
      <param name="base_frame" value="SVEA1" />
    </node>
</launch>
